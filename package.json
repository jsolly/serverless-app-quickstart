{
	"name": "app-quickstart",
	"type": "module",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"prepare": "if [ -z \"$CI\" ]; then husky; fi",
		"test:unit": "vitest --no-watch -t 'unit'",
		"check:ts": "tsc --noEmit",
		"check:iac": "terraform validate",
		"check:fix": "biome check --write --no-errors-on-unmatched",
		"fix": "npm run check:fix && npm run check:ts",
		"outdated": "npm outdated",
		"update": "npx npm-check-updates -u && npm install"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,css,md,yml,yaml}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/ts-plugin": "^1.10.4",
		"@astrojs/sitemap": "^3.6.0",
		"@biomejs/biome": "2.2.4",
		"@tailwindcss/vite": "^4.1.14",
		"astro": "^5.14.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.3",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.3",
		"vitest": "^3.1.4"
	}
}
